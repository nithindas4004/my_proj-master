<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.noAction">
        <item name="android:windowActionBar">true</item>-->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowActivityTransitions">true</item>

    </style>

    <style name="AppTheme.statusAction">
        <!--<item name="android:windowLightStatusBar">true</item>-->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

    </style>
    <style name="abc"  parent="ThemeOverlay.AppCompat.Dark.ActionBar">

        <item name="colorControlNormal">@color/white</item>
    </style>
    <style name="AppTheme.statusAction.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.statusAction.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
